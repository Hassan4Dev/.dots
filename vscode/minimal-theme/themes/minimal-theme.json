{
  "name": "Minimal Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#151515",
    "editor.foreground": "#e0e0e0",
    "editorLineNumber.foreground": "#353535ff",
    "editorLineNumber.activeForeground": "#a9a9a9ff",
    "editorGroupHeader.tabsBackground": "#151515",
    "editorInlayHint.parameterBackground": "#0000",
    "editor.findMatchBackground": "#ffffff14",
    "editor.findMatchHighlightBackground": "#ffffff14",
    "editor.wordHighlightBackground": "#ffffff14",
    "editor.wordHighlightStrongBackground": "#ffffff14",
    "editor.selectionBackground": "#ffffff1a",
    "editor.foldBackground": "#0000",

    "editorBracketHighlight.foreground1": "#555555",
    "editorBracketHighlight.foreground2": "#666666",
    "editorBracketHighlight.foreground3": "#777777",
    "editorBracketHighlight.foreground4": "#888888",
    "editorBracketHighlight.foreground5": "#999999",
    "editorBracketHighlight.foreground6": "#aaaaaa",

    "sideBar.background": "#151515",
    "sideBar.foreground": "#a0a0a0",
    "sideBar.border": "#202020",

    "activityBar.background": "#151515",
    "activityBar.foreground": "#a0a0a0",

    "tab.activeBorder": "#e0e0e0",
    "tab.activeBackground": "#151515",
    "tab.inactiveBackground": "#151515",
    "tab.activeForeground": "#e0e0e0",
    "tab.inactiveForeground": "#5a5a5a",
    "tab.unfocusedInactiveForeground": "#505050",

    "list.activeSelectionBackground": "#1f1f1f",
    "list.activeSelectionForeground": "#f0f0f0",
    "list.inactiveSelectionBackground": "#1f1f1f",
    "list.hoverBackground": "#1a1a1a",
    "list.highlightForeground": "#f0f0f0",
    "list.focusBackground": "#ffffff10",
    "list.focusOutline": "#0000",
    "list.errorForeground": "#d06060",

    "input.background": "#1a1a1a",
    "input.foreground": "#f0f0f0",

    "diffEditor.insertedTextBackground": "#ffffff08",
    "diffEditor.removedTextBackground": "#00000033",

    "scrollbarSlider.background": "#30303080",

    "textLink.foreground": "#b0b0b0",

    "terminal.ansiYellow": "#c0c0c0",
    "terminal.ansiCyan": "#a0a0a08b",

    "gitDecoration.modifiedResourceForeground": "#eaeaeaff",
    "gitDecoration.untrackedResourceForeground": "#eaeaeaff",

    "quickInput.background": "#151515",
    "quickInput.foreground": "#e0e0e0",
    "quickInputList.focusBackground": "#ffffff10",
    "quickInputList.focusForeground": "#f0f0f0",
    "descriptionForeground": "#b0b0b0",
    "pickerGroup.foreground": "#b0b0b0"
  },

  "tokenColors": [
    {
      "scope": [
        "string",
        "string.template",
        "meta.tag.attributes string",
        "meta.tag.attributes punctuation.definition.string",
        "punctuation.definition.string",
        "meta.object.member string",
        "meta.object.member punctuation.definition.string",
        "punctuation.definition.string.template"
      ],
      "settings": { "foreground": "#F6B98B" }
    },
    {
      "scope": [
        "variable",
        "variable.property",
        "support.type.property-name",
        "support.variable.property",
        "constant.language.import-export-all",
        "meta.import string",
        "meta.import punctuation.definition.string",
        "meta.export string",
        "meta.export punctuation.definition.string"
      ],
      "settings": { "foreground": "#E6E6E6" }
    },
    {
      "scope": "constant",
      "settings": { "foreground": "#9CE9CE" }
    },
    {
      "scope": ["entity.name", "support.class", "support.type"],
      "settings": { "foreground": "#A3D4E7" }
    },
    {
      "scope": [
        "storage.type",
        "storage.modifier",
        "keyword",
        "punctuation",
        "meta.brace",
        "entity.other",
        "variable.language.this"
      ],
      "settings": { "foreground": "#9ea1a4e2" }
    },
    {
      "scope": [
        "entity.name.tag",
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": { "foreground": "#E6E7A3" }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#46474F" }
    }
  ]
}
